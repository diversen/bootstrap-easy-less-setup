{
  "name": "bootstrap-less-easy-setup",
  "version": "1.0.9",
  "description": "Simple dev setup of bootstrap. Compile bootstrap's less files to CSS on changes to the less files. ",
  "main": "index.js",
  "scripts": {
    "watch": "simple-file-watch --extension='less' --path='./public' --command='lessc --source-map=public/styles.map public/styles.less public/styles.css'",
    "serve": "http-server public",
    "setup": "./setup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diversen/bootstrap-easy-less-setup.git"
  },
  "keywords": [
    "bootstrap",
    "source mappings",
    "less",
    "easy",
    "setup"
  ],
  "author": "dennis iversen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diversen/bootstrap-easy-less-setup"
  },
  "homepage": "https://github.com/diversen/bootstrap-easy-less-setup",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "http-server": "^0.9.0",
    "simple-file-watch": "^2.0.0"
  },
  "devDependencies": {
    "replace": "^0.3.0"
  }
}
